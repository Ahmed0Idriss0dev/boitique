
generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
  directUrl = env("DIRECT_URL")

}
model Products {
  id                String   @id @unique @default(cuid())
  userId            String?
  title             String?
  description       String?
  price             Int
  Delivery          Boolean? @default(true)
  ProductsImageList String[]
}

model Orders {
  id         String   @id @unique @default(cuid())
  userId     String?
  productsId String[]
  phone      Int?
  fullname    String?
  location   Boolean?
}